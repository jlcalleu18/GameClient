<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.MenuBar?>
<AnchorPane prefHeight="698.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.citytech.properties.PropertiesController">
   <children>
      <TableView fx:id="tvProperties" layoutX="21.0" layoutY="60.0" prefHeight="608.0" prefWidth="519.0">
        <columns>
          <TableColumn fx:id="tcPropertyId" text="Property Id">
              <cellValueFactory>
                  <PropertyValueFactory property="propertyId" />
              </cellValueFactory>
          </TableColumn>
          <TableColumn fx:id="tcCost" prefWidth="86.0" text="Cost">
              <cellValueFactory>
                  <PropertyValueFactory property="cost" />
              </cellValueFactory>
          </TableColumn>
            <TableColumn fx:id="tcGrossIncome" prefWidth="85.0" text="Gross Income">
                <cellValueFactory>
                    <PropertyValueFactory property="grossIncome" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="tcEpenses" prefWidth="87.0" text="Expenses">
                <cellValueFactory>
                    <PropertyValueFactory property="expenses" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="tcNetIncome" prefWidth="93.0" text="Net Income">
                <cellValueFactory>
                    <PropertyValueFactory property="netincome" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="tcOccupancy" prefWidth="76.0" text="Occupancy">
                <cellValueFactory>
                    <PropertyValueFactory property="occupancy" />
                </cellValueFactory>
            </TableColumn>
        </columns>
      </TableView>
      <Label layoutX="227.0" layoutY="14.0" text="Properties">
         <font>
            <Font name="System Bold" size="25.0" />
         </font></Label>
       <MenuBar layoutX="1.0" layoutY="1.0">
           <menus>
               <Menu mnemonicParsing="false" text="Display Mode">
                   <items>
                       <MenuItem mnemonicParsing="false" onAction="#tictactoeMode" text="TicTacToe" />
                       <MenuItem mnemonicParsing="false" onAction="#abcCounterMode" text="ABC Counter" />
                       <MenuItem mnemonicParsing="false" onAction="#growthRateMode" text="Growth Rate" />
                       <MenuItem mnemonicParsing="false" onAction="#retirementMode" text="Retirement" />
                       <MenuItem mnemonicParsing="false" onAction="#propertiesMode" text="Properties" />
                   </items>
               </Menu>
           </menus>
       </MenuBar>
   </children>
</AnchorPane>
