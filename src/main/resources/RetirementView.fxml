<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="650.0" prefWidth="571.0" stylesheets="@retirement.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.citytech.retirement.RetirementController">
   <children>
      <Label layoutX="256.0" layoutY="14.0" text="Retirement" />
      <TableView fx:id="tvRetirement" layoutX="26.0" layoutY="95.0" prefHeight="506.0" prefWidth="520.0">
        <columns>
          <TableColumn prefWidth="33.0" text="No">
              <cellValueFactory>
                  <PropertyValueFactory property="no" />
              </cellValueFactory>
          </TableColumn>
          <TableColumn fx:id="tcStartingBalance" prefWidth="105.0" text="Starting Balance">
            <cellValueFactory>
                <PropertyValueFactory property="startingBalance" />
            </cellValueFactory>
          </TableColumn>
            <TableColumn fx:id="tcYearlyDeposit" prefWidth="111.0" text="Yearly Deposit">
                <cellValueFactory>
                    <PropertyValueFactory property="yearlyDeposit" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="tcInterestEarned" prefWidth="117.0" text="Interest Earned">
                <cellValueFactory>
                    <PropertyValueFactory property="interestEarned" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="tcEndingBalance" prefWidth="136.0" text="Ending Balance">
                <cellValueFactory>
                    <PropertyValueFactory property="endingBalance" />
                </cellValueFactory>
            </TableColumn>
        </columns>
      </TableView>
       <MenuBar layoutX="1.0" layoutY="1.0">
           <menus>
               <Menu mnemonicParsing="false" text="Display Mode">
                   <items>
                       <MenuItem mnemonicParsing="false" onAction="#tictactoeMode" text="TicTacToe" />
                       <MenuItem mnemonicParsing="false" onAction="#abcCounterMode" text="ABC Counter" />
                       <MenuItem mnemonicParsing="false" onAction="#growthRateMode" text="Growth Rate" />
                       <MenuItem mnemonicParsing="false" onAction="#retirementMode" text="Retirement" />
                   </items>
               </Menu>
           </menus>
       </MenuBar>
      <TextField fx:id="txtYears" layoutX="77.0" layoutY="59.0" prefHeight="25.0" prefWidth="90.0" />
      <Label layoutX="26.0" layoutY="63.0" text="years" />
      <Button layoutX="225.0" layoutY="59.0" mnemonicParsing="false" onAction="#process" text="Process" />
      <Label layoutX="26.0" layoutY="619.0" text="Calle, Jorge" />
   </children>
</AnchorPane>
